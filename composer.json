{
    "name": "orchestra/tenanti",
    "description": "Tenant based Database Schema Manager for Laravel",
    "keywords": ["laravel", "orchestral", "orchestra-platform", "multi-tenant", "schema", "migration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Tenanti\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tenanti\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Kernel.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "illuminate/console": "~5.8.0",
        "illuminate/filesystem": "~5.8.0",
        "orchestra/model": "~3.8.0",
        "orchestra/support": "~3.8.0"
    },
    "require-dev": {
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^7.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.8-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
